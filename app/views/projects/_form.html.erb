        </div>

        <div class="md-form">
          <%= form.label :notes %>
          <%= form.text_area :notes, id: :project_notes, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :user_id %>
          <%= form.text_field :user_id, id: :project_user_id, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :company_id %>
          <%= form.text_field :company_id, id: :project_company_id, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :contact_id %>
          <%= form.text_field :contact_id, id: :project_contact_id, class: "form-control" %>
        </div>


      </div>
    <% end %>
  </div>
</div>
