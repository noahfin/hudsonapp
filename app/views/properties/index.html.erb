<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street num</th>
      <th>Street name</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Zipcode ext</th>
      <th>County</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Address</th>
      <th>Listing broker</th>
      <th>Available</th>
      <th>On market</th>
      <th>Expired listing</th>
      <th>Contact</th>
      <th>User</th>
      <th>Contact</th>
      <th>Marketing</th>
      <th>Campaign</th>
      <th>Todo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.name %></td>
        <td><%= property.street_num %></td>
        <td><%= property.street_name %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zip %></td>
        <td><%= property.zipcode_ext %></td>
        <td><%= property.county %></td>
        <td><%= property.latitude %></td>
        <td><%= property.longitude %></td>
        <td><%= property.address %></td>
        <td><%= property.listing_broker %></td>
        <td><%= property.available %></td>
        <td><%= property.on_market %></td>
        <td><%= property.expired_listing %></td>
        <td><%= property.contact %></td>
        <td><%= property.user %></td>
        <td><%= property.contact %></td>
        <td><%= property.marketing %></td>
        <td><%= property.campaign %></td>
        <td><%= property.todo %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
