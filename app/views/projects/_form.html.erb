<div class="container">

<div class="row">

    <div class="col-md-6">


    <%= form_with(model: project, local: true) do |form| %>
      <% if project.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(project.errors.count, "error") %> prohibited this project from being saved:</h2>

          <ul>
          <% project.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>

        <div class="md-form">
          <%= form.label :street_number %>
          <%= form.text_field :street_number, id: :project_street_number, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :street_name %>
          <%= form.text_field :street_name, id: :project_street_name, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :city %>
          <%= form.text_field :city, id: :project_city, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :state %>
          <%= form.text_field :state, id: :project_state, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :zipcode %>
          <%= form.text_field :zipcode, id: :project_zipcode, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :zipcode_ext %>
          <%= form.text_field :zipcode_ext, id: :project_zipcode_ext, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :county %>
          <%= form.text_field :county, id: :project_county, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :latitude %>
          <%= form.text_field :latitude, id: :project_latitude, class: "form-control" %>
        </div>

        <div class="md-form">
          <%= form.label :property_address %>
          <%= form.text_field :property_address, id: :project_property_address, class: "form-control" %>
        </div>
      </div>

      <div class="col-md-6">

        <div class="md-form">
