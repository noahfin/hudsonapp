<p id="notice"><%= notice %></p>

<h1>Compaines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone number</th>
      <th>Phone number 2</th>
      <th>Fax</th>
      <th>Website</th>
      <th>Owns</th>
      <th>Rents</th>
      <th>Primary idustry</th>
      <th>Secondary idustry</th>
      <th>Sf occupying</th>
      <th>Email</th>
      <th>Notes</th>
      <th>Sic code</th>
      <th>Data checked</th>
      <th>Type of location</th>
      <th>Listing agent</th>
      <th>Suit</th>
      <th>Total employees</th>
      <th>Year founded</th>
      <th>Follow up</th>
      <th>Active</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compaines.each do |compaine| %>
      <tr>
        <td><%= compaine.name %></td>
        <td><%= compaine.address %></td>
        <td><%= compaine.phone_number %></td>
        <td><%= compaine.phone_number_2 %></td>
        <td><%= compaine.fax %></td>
        <td><%= compaine.website %></td>
        <td><%= compaine.owns %></td>
        <td><%= compaine.rents %></td>
        <td><%= compaine.primary_idustry %></td>
        <td><%= compaine.secondary_idustry %></td>
        <td><%= compaine.SF_occupying %></td>
        <td><%= compaine.email %></td>
        <td><%= compaine.notes %></td>
        <td><%= compaine.sic_code %></td>
        <td><%= compaine.data_checked %></td>
        <td><%= compaine.type_of_location %></td>
        <td><%= compaine.listing_agent %></td>
        <td><%= compaine.suit %></td>
        <td><%= compaine.total_employees %></td>
        <td><%= compaine.year_founded %></td>
        <td><%= compaine.follow_up %></td>
        <td><%= compaine.active %></td>
        <td><%= compaine.user %></td>
        <td><%= link_to 'Show', compaine %></td>
        <td><%= link_to 'Edit', edit_compaine_path(compaine) %></td>
        <td><%= link_to 'Destroy', compaine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compaine', new_compaine_path %>
